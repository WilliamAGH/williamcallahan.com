{
  "name": "william-callahan-dot-com",
  "type": "module",
  "private": true,
  "version": "0.0.6",
  "homepage": "https://williamcallahan.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WilliamAGH/williamcallahan.com.git"
  },
  "bugs": {
    "url": "https://github.com/WilliamAGH/williamcallahan.com/issues"
  },
  "scripts": {
    "update-s3": "bun scripts/data-updater.ts",
    "scheduler": "bun lib/server/scheduler.ts",
    "submit-sitemap": "bun scripts/submit-sitemap.ts",
    "analyze": "NODE_OPTIONS='--max-old-space-size=4096' ANALYZE=true next build",
    "prestart": "",
    "prefetch": "bun scripts/data-updater.ts --bookmarks --github --logos",
    "populate-data": "NODE_ENV=development bun scripts/data-updater.ts --bookmarks --github --logos",
    "dev": "bun run cleanCache && (NODE_ENV=development bun scripts/data-updater.ts --quiet &) && NODE_OPTIONS='--max-old-space-size=4096 --max-semi-space-size=256' NODE_ENV=development next dev",
    "dev:update-data": "NODE_ENV=development bun scripts/data-updater.ts",
    "dev:with-data": "bun run populate-data && bun run cleanCache && NODE_OPTIONS='--max-old-space-size=4096 --max-semi-space-size=256' NODE_ENV=development next dev",
    "dev:full": "bun run populate-data && bun run cleanCache && NODE_OPTIONS='--max-old-space-size=4096 --max-semi-space-size=256' NODE_ENV=development next dev",
    "dev:turbo": "bun run cleanCache && NODE_OPTIONS='--max-old-space-size=4096 --max-semi-space-size=256' NODE_ENV=development next dev --turbo",
    "dev:memory-debug": "bun run cleanCache && NODE_OPTIONS='--max-old-space-size=4096 --max-semi-space-size=256 --expose-gc' NODE_ENV=development next dev",
    "build": "bun run validate && NODE_OPTIONS='--max-old-space-size=4096' next build",
    "build:only": "bun run validate && NODE_OPTIONS='--max-old-space-size=4096' next build",
    "build:full": "bun run validate && NODE_OPTIONS='--max-old-space-size=4096' next build",
    "start": "NODE_OPTIONS='--max-old-space-size=4096' node .next/standalone/server.js",
    "start:plain": "NODE_OPTIONS='--max-old-space-size=4096' next start",
    "docker:build": "docker buildx build --platform=linux/amd64 --build-arg NODE_ENV=production --build-arg NEXT_TELEMETRY_DISABLED=1 --load -t williamcallahan-web:latest .",
    "docker:start": "docker run --platform=linux/amd64 -p 3010:3000 -e NODE_ENV=production -e NODE_OPTIONS='--max-old-space-size=4096' williamcallahan-web:latest",
    "docker:prefetch": "docker run --platform=linux/amd64 -p 3010:3000 -e NODE_ENV=production -e CONTAINER=true williamcallahan-web:latest bun scripts/data-updater.ts --bookmarks --github --logos",
    "docker:populate-data": "docker run --platform=linux/amd64 -p 3010:3000 -e NODE_ENV=production -e CONTAINER=true williamcallahan-web:latest bun scripts/data-updater.ts --bookmarks --github --logos",
    "monitor:status": "bun lib/health/status-monitor.server.ts",
    "lint": "eslint . --config eslint.config.ts --format compact",
    "lint:errors": "eslint . --config eslint.config.ts --quiet --format compact",
    "lint:css": "stylelint --fix 'app/**/*.css' 'styles/**/*.css'",
    "biome:check": "biome check .",
    "biome:format": "biome format --write .",
    "biome:lint": "biome lint .",
    "type-check": "NODE_OPTIONS='--max-old-space-size=4096' tsc --noEmit",
    "type-check:tests": "NODE_OPTIONS='--max-old-space-size=4096' tsc --noEmit --project __tests__/tsconfig.json",
    "check:file-size": "bash -c 'err=0; for f in $(find . -type f \\( -name \"*.ts\" -o -name \"*.tsx\" \\) ! -name \"*.d.ts\" | grep -v \"node_modules/\" | grep -v \".next/\"); do lines=$(wc -l < \"$f\"); if [ $lines -gt 500 ]; then echo \"$f exceeds $lines lines\"; err=1; fi; done; exit $err'",
    "validate": "bun run lint && bun run type-check && bun run biome:lint && bun scripts/analyze-circular-deps.ts",
    "validate:with-size": "bun run validate && bun run check:file-size",
    "validate:full": "bun run lint && bun run type-check && bun run biome:check",
    "clean": "rm -rf .next out node_modules",
    "cleanCache": "rm -rf .next/cache",
    "test": "NODE_OPTIONS='--max-old-space-size=4096' jest --color --config=config/jest/config.ts",
    "test:watch": "NODE_OPTIONS='--max-old-space-size=4096' jest --color --watch --config=config/jest/config.ts",
    "test:coverage": "NODE_OPTIONS='--max-old-space-size=4096' jest --coverage --config=config/jest/config.ts",
    "test:ci": "NODE_OPTIONS='--max-old-space-size=4096' jest --ci --coverage --config=config/jest/config.ts",
    "test:leaks": "NODE_OPTIONS='--max-old-space-size=4096' jest --detectOpenHandles --config=config/jest/config.ts",
    "test:smoke": "NODE_OPTIONS='--max-old-space-size=4096' jest --testPathPatterns=smoke --config=config/jest/config.ts",
    "test:smoke:blog": "bun run test:smoke -- --testPathPatterns=blog",
    "test:smoke:pages": "bun run test:smoke -- --testPathPatterns=pages",
    "prepare": "node .husky/install.mjs",
    "validate-opengraph": "bun scripts/validate-opengraph-clear-cache.ts",
    "postdeploy": "bun run validate-opengraph"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.840.0",
    "@aws-sdk/lib-storage": "3.840.0",
    "@fontsource/ibm-plex-mono": "^5.2.6",
    "@mapbox/rehype-prism": "^0.9.0",
    "@mdx-js/mdx": "^3.1.0",
    "@octokit/graphql": "^8.2.2",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/instrumentation": "0.202.0",
    "@sentry/browser": "9.33.0",
    "@sentry/nextjs": "9.33.0",
    "@sentry/opentelemetry": "9.33.0",
    "@tailwindcss/typography": "^0.5.16",
    "cheerio": "^1.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "file-type": "^21.0.0",
    "framer-motion": "^12.20.1",
    "fuse.js": "^7.1.0",
    "gaxios": "^7.1.1",
    "google-auth-library": "^10.1.0",
    "gray-matter": "^4.0.3",
    "hoist-non-react-statics": "^3.3.2",
    "import-in-the-middle": "1.14.0",
    "lucide-react": "0.525.0",
    "mdast-util-mdx-jsx": "^3.2.0",
    "minisearch": "^7.1.2",
    "next": "canary",
    "next-mdx-remote": "4.4.1",
    "next-themes": "^0.4.6",
    "node-cron": "4.2.0",
    "prismjs": "^1.30.0",
    "react": "^19.1.0",
    "react-activity-calendar": "^2.7.13",
    "react-dom": "^19.1.0",
    "react-parallax-tilt": "1.7.299",
    "react-tweet": "^3.2.2",
    "rehype-prism": "^2.3.3",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^2.0.0",
    "remark-mdx": "^3.1.0",
    "require-in-the-middle": "^7.5.2",
    "schema-dts": "^1.1.5",
    "server-only": "^0.0.1",
    "swr": "2.3.3",
    "systeminformation": "^5.27.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "use-sync-external-store": "^1.5.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@eslint/js": "9.30.0",
    "@happy-dom/global-registrator": "18.0.1",
    "@mdx-js/react": "^3.1.0",
    "@next/eslint-plugin-next": "15.3.4",
    "@svgr/webpack": "^8.1.0",
    "@swc/core": "1.12.7",
    "@testing-library/jest-dom": "^6.6.3",
