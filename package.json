    "tailwindcss": "^3.4.17",
    "ts-jest": "29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "undici": "^7.11.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mdx}": [
      "eslint --config eslint.config.ts --fix"
    ]
  },
  "trustedDependencies": [
    "@sentry/cli",
