{
  "name": "william-callahan-dot-com",
  "private": true,
  "version": "0.0.2",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "pnpm run validate && next build",
    "start": "node .next/standalone/server.js",
    "start:plain": "next start",
    "docker:build": "docker buildx build --platform=linux/amd64 --build-arg NODE_ENV=production --build-arg NEXT_TELEMETRY_DISABLED=1 --load -t williamcallahan-web:latest .",
    "docker:start": "docker run --platform=linux/amd64 -p 3010:3000 -e NODE_ENV=production williamcallahan-web:latest",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "validate": "pnpm run lint && pnpm run type-check",
    "clean": "rm -rf .next out node_modules",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "prepare": "node .husky/install.mjs"
  },
  "dependencies": {
    "@sentry/nextjs": "^8.55.0",
    "@tailwindcss/typography": "^0.5.15",
    "clsx": "^2.1.1",
    "fuse.js": "^7.0.0",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.344.0",
    "next": "^15.2.4",
    "next-mdx-remote": "^4.4.1",
    "next-themes": "^0.2.1",
    "node-cache": "^5.1.2",
    "require-in-the-middle": "^7.5.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rehype-prism": "^2.3.3",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^3.0.1",
    "schema-dts": "^1.1.2",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@mdx-js/react": "^3.1.0",
    "@svgr/webpack": "^8.1.0",
    "@swc/core": "^1.10.1",
    "@swc/jest": "^0.2.37",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.24",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@typescript-eslint/typescript-estree": "^8.18.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.21",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "node-fetch": "^2.7.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2"
  }
}
